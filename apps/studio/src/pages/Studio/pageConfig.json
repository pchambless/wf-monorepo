{
  "layout": "flex",
  "components": [
    {
      "id": "studioPage",
      "comp_type": "Page",
      "container": "Page",
      "position": {},
      "props": {},
      "workflowTriggers": {
        "onLoad": [
          {"ordr": 1, "action": "refresh", "content": "[\"selectApp\"]", "is_dom_event": 1}
        ]
      },
      "components": [
        {
          "id": "studioSidebar",
          "comp_type": "Sidebar",
          "container": "Column",
          "position": {"row": {"start": 1, "span": 10}, "col": {"start": 1, "span": 2}},
          "override_styles": {"padding": "16px", "gap": "12px", "display": "grid", "gridTemplateColumns": "1fr", "gridTemplateRows": "auto"},
          "props": {},
          "components": [
            {
              "id": "selectApp",
              "comp_type": "Select",
              "container": "inline",
              "position": {"row": {"start": 1, "span": 1}, "col": {"start": 1, "span": 1}},
              "override_styles": {"width": "100%"},
              "props": {},
              "workflowTriggers": {
                "onChange": [
                  {
                    "ordr": 2,
                    "action": "setVals",
                    "content": "\"appID\": \"{{this.value}}\", \"appName\": \"{{this.selectedOptions[0].text}}\"",
                    "is_dom_event": 1
                  },
                  {"ordr": 3, "action": "refresh", "content": "[\"selectPage\"]", "is_dom_event": 1}
                ],
                "onRefresh": [
                  {"ordr": 1, "action": "execEvent", "content": "appList", "is_dom_event": 0}
                ]
              }
            },
            {
              "id": "selectPage",
              "comp_type": "Select",
              "container": "inline",
              "position": {"row": {"start": 2, "span": 1}, "col": {"start": 1, "span": 1}},
              "override_styles": {"width": "100%"},
              "props": {},
              "workflowTriggers": {
                "onChange": [
                  {
                    "ordr": 2,
                    "action": "setVals",
                    "content": "\"pageID\": \"{{this.value}}\", \"pageName\": \"{{this.selectedOptions[0].text}}\"",
                    "is_dom_event": 1
                  },
                  {
                    "ordr": 3,
                    "action": "refresh",
                    "content": "[\"chartMermaid\"]",
                    "is_dom_event": 1
                  },
                  {"ordr": 4, "action": "clearVals", "content": "eventTypeID", "is_dom_event": 1}
                ],
                "onRefresh": [
                  {"ordr": 1, "action": "execEvent", "content": "pageList", "is_dom_event": 0}
                ]
              }
            },
            {
              "id": "previewPage",
              "comp_type": "Button",
              "container": "inline",
              "position": {"row": {"start": 3, "span": 1}, "col": {"start": 1, "span": 1}},
              "override_styles": {"width": "100%"},
              "props": {},
              "workflowTriggers": {
                "onClick": [
                  {
                    "ordr": 1,
                    "action": "openModal",
                    "content": "{\"modalId\": \"pagePreview\"}",
                    "is_dom_event": 1
                  }
                ]
              }
            }
          ]
        },
        {
          "id": "studioCanvasArea",
          "comp_type": "StudioCanvasWrapper",
          "container": "Column",
          "position": {"row": {"start": 1, "span": 20}, "col": {"start": 3, "span": 10}},
          "props": {}
        },
        {
          "id": "pagePreview",
          "comp_type": "Page",
          "container": "Modal",
          "position": {},
          "props": {},
          "workflowTriggers": {
            "onRefresh": [
              {"ordr": 1, "action": "execEvent", "content": "xrefHierarchy", "is_dom_event": 0}
            ]
          },
          "components": [
            {
              "id": "previewFrame",
              "comp_type": "iframe",
              "container": "inline",
              "position": {"row": {"start": 1, "span": 12}, "col": {"start": 1, "span": 12}},
              "props": {
                "src": "http://localhost:3004/preview/{{context.appName}}/{{context.pageName}}",
                "width": "100%",
                "height": "800px",
                "frameBorder": 0
              }
            }
          ]
        }
      ]
    }
  ],
  "title": "studio",
  "routePath": "/monorepo/studio",
  "purpose": "Database-generated page configuration",
  "cluster": "Page"
}