{
  "mcpServers": {
    "mysql": {
      "command": "bash",
      "args": [
        "-c",
        "source ~/.wf-db-credentials && exec npx -y @liangshanli/mcp-server-mysql"
      ],
      "disabled": false,
      "autoApprove": [
        "sql_query"
      ]
    },
    "n8n-workflows": {
      "command": "bash",
      "args": [
        "-c",
        "source /home/paul/Projects/wf-monorepo/.env && exec npx -y supergateway --streamableHttp http://0.0.0.0:5678/mcp-server/http --header \"authorization:Bearer $N8N_API_KEY\""
      ],
      "disabled": false,
      "autoApprove": []
    },
    "docker-toolkit": {
      "command": "docker",
      "args": [
        "mcp",
        "gateway",
        "run"
      ],
      "disabled": false,
      "autoApprove": []
    }
  }
}